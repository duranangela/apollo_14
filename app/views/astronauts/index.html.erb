<h1>All Astronauts</h1>
<h2>Average age = <%= @average_age %></h2>

<ul>
  <% @astronauts.each do |astronaut| %>
    <li><%= astronaut.name %></li>
    <li><%= astronaut.age %></li>
    <li><%= astronaut.job %></li>
    <li>Missions
      <ul>
      <% astronaut.space_missions.each do |mission| %>
      <li><%= mission.title %></li>
      <% end %>
    </ul></li><br/><br/>
  <% end %>
</ul>
